<Window x:Class="WPFButtons.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFButtons"
        mc:Ignorable="d"
        Title="MainWindow" Height="275" Width="260">
    <Canvas>
        <Ellipse Panel.ZIndex="0" Fill="black" Canvas.Left="30" Canvas.Top="30" Width="200" Height="200"/>
        <Rectangle Panel.ZIndex="1" Fill="Blue" Canvas.Top="25" Canvas.Left="25" Width="50" Height="50"/>
        <Rectangle Panel.ZIndex="2" Fill="#FF00FFBF"  Canvas.Top="50" Canvas.Left="50" Width="50" Height="50"/>
        <Rectangle Panel.ZIndex="1" Fill="green" Canvas.Top="75" Canvas.Left="75" Width="50" Height="50"/>


    </Canvas>
</Window>
